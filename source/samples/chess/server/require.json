{
    "EXE": [
        "mkdir -p server/media server/css server/js/handlers server/js/workers",
        "cp source/samples/chess/server/media/* server/media"
    ],
    "source/samples/chess/server/*.html": "server -vars=vars.json -options=placeholderMode:'func'",
    "source/samples/chess/server/less/*.less": "server/css -plugins=malta-less -options=placeholderMode:'func'",
    "source/samples/chess/server/js/*.js": "server/js -plugins=malta-header-comment[name:\"source/header.txt\"] -vars=vars.json -options=placeholderMode:'func'",
    "source/samples/chess/server/js/workers/*.js": "server/js/workers -plugins=malta-header-comment[name:\"source/header.txt\"] -vars=vars.json -options=placeholderMode:'func'",
    "source/samples/chess/server/js/handlers/*.js": "server/js/handlers -plugins=malta-header-comment[name:\"source/header.txt\"] -vars=vars.json -options=placeholderMode:'func'"
}