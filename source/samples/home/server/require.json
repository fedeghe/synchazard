{
    "EXE": [
        "mkdir -p server/css server/js/handlers server/js/lib server/js/workers server/media",
        "cp source/samples/home/server/media/* server/media/"
    ],
    "source/samples/home/server/*.html": "server -vars=vars.json",
    "source/samples/home/server/less/*.less": "server/css -plugins=malta-less",
    "source/samples/home/server/js/*.js": "server/js -plugins=malta-header-comment[name:\"source/header.txt\"] -vars=vars.json",
    "source/samples/home/server/js/workers/*.js": "server/js/workers -plugins=malta-header-comment[name:\"source/header.txt\"] -vars=vars.json",
    "source/samples/home/server/js/handlers/*.js": "server/js/handlers -plugins=malta-header-comment[name:\"source/header.txt\"] -vars=vars.json"
}