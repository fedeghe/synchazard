{
    "EXE": [
        "mkdir -p serverws/{core,actions,js/lib}"
    ],
    "source/serverws/*.js": "serverws -vars=vars.json -options=placeholderMode:'func'",
    "source/serverws/core/*.js": "serverws/core -vars=vars.json -options=placeholderMode:'func'",
    "source/serverws/WSSERVER.md": "serverws -plugins=malta-rename[to:'README.md'] -vars=vars.json -options=placeholderMode:'func'"
}